{
  "manifest_version": 3,
  "name": "Internet to Caspar",
  "version": "0.3",
  "description": "Right click to send image to Caspar",
  "icons": {
    "16": "./assets/casparcg [16x16].png",
    "32": "./assets/casparcg [32x32].png",
    "64": "./assets/casparcg [64x64].png",
    "280": "./assets/casparcg.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus", "activeTab", "scripting"]
}